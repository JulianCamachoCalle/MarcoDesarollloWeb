package com.example.pruebavalidar.DAO;

import com.example.pruebavalidar.javaentity.ContactoDp;
import java.util.ArrayList;


public class ContactoDAO {
    ArrayList<ContactoDp> listaCont=new ArrayList<>();
    public void registrarCont(ContactoDp objAE){
        listaCont.add(objAE);
    }
    public ArrayList<ContactoDp> getcontacto(){
        return listaCont;
    }
}
